<config>
	<jars>
		<jar in="RSBot.jar" out="RSBot.jar" />
	</jars>
	<keep-names>
		<class template="class org.powerbot.Boot">
			<method template="public main(java.lang.String[])" />
		</class>
		<class template="class org.powerbot.bot.Bot" />
		<class template="class org.powerbot.bot.client.*">
			<field access="protected+" />
			<method access="protected+" />
		</class>
		<class template="class org.powerbot.bot.loader.*">
			<field access="public" />
			<method access="public" />
		</class>
		<class template="class org.powerbot.event.*">
			<field access="protected+" />
			<method access="protected+" />
		</class>
		<class template="class org.powerbot.script.internal.*" stop="true" />
		<class template="class org.powerbot.script.*">
			<field access="protected+" />
			<method access="protected+" parameters="keep" />
		</class>
	</keep-names>
	<property name="log-file" value="allatori-log.xml" />
	<property name="string-encryption" value="maximum" />
	<property name="string-encryption-type" value="strong" />
	<property name="extensive-flow-obfuscation" value="maximum" />
	<property name="default-package" value="org.powerbot" />
	<property name="classes-naming" value="windows" />
	<property name="methods-naming" value="keywords" />
	<property name="fields-naming" value="keywords" />
	<property name="line-numbers" value="remove" />
</config>
